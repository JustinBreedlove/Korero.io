{
  "name": "korrero",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "cd ./api && npm install && cd ../client && npm install && npm run build",
    "start": "concurrently \"npm run api\" \"npm run client\"",
    "api": "cd ./api && node ./bin/www",
    "client": "cd ./client && npm start",
    "test": "cd ./client && npx mocha test"
  },
  "dependencies": {
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "middleware": "^1.0.0",
    "mocha": "^10.2.0",
    "mongodb": "^5.1.0",
    "supertest": "^6.3.3"
  }
}
