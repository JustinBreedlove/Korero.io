{
  "name": "korrero",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build":"concurrently \"cd ./api && npm run build\" \"cd ./client && npm run build\"",
    "start": "concurrently \"npm run api\" \"npm run client\"",
    "api": "cd ./api && node ./bin/www",
    "client": "cd ./client && npm start",
    "test": "concurrently \"cd ./api && npx mocha test\" \"cd ./api && npx mocha test\""
  },
  "dependencies": {
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "middleware": "^1.0.0",
    "mocha": "^10.2.0",
    "mongodb": "^5.1.0",
    "morgan": "^1.10.0",
    "web-vitals": "^3.3.1",
    "pm2": "^5.3.0",
    "nodemon": "^2.0.22"
  }
}
